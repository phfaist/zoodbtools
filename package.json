{
  "name": "zoodbeditobject",
  "version": "0.1.0",
  "description": "Simple React app to edit objects in a ZooDB database",
  "repository": "https://github.com/phfaist/zoodbeditobject",
  "author": "Philippe Faist",
  "license": "MIT",
  "dependencies": {
    "@codemirror/language": "^6.0.0",
    "@codemirror/legacy-modes": "^6.0.0",
    "@uiw/react-codemirror": "^4.15.1",
    "jsonschema": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tabs": "^6.0.0"
  },
  "peerDependencies": {
    "@phfaist/zoodb": "*"
  },
  "devDependencies": {
    "@kkirbatski/parcel-resolver-require-resolve": "^1.0.4",
    "@mischnic/parcel-resolver-root": "^0.4.2",
    "@parcel/config-default": "^2.8.3",
    "@parcel/optimizer-data-url": "^2.8.3",
    "@parcel/transformer-inline-string": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@phfaist/zoodb": "link:../zoodb",
    "assert": "^2.0.0",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "sirv-cli": "^2.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "util": "^0.12.5"
  },
  "scripts": {
    "dev": "parcel --no-cache playground/mytest.html",
    "dev-build": "rm -rf dist && parcel build --no-optimize --no-cache playground/mytest.html && yarn private-dist-add-use-strict",
    "serve": "sirv dist/",
    "private-dist-add-use-strict": "bash -c 'for i in dist/*.js; do if [ -e \"$i\" ]; then mv \"$i\" \"$i\".COPY; echo \"\\\"use strict\\\";\" >\"$i\"; cat \"$i\".COPY >>\"$i\"; rm \"$i\".COPY; echo \"added “use strict” to $i\"; fi; done'"
  },
  "alias": {
    "process": false,
    "stream": "./node_modules/stream-browserify/index.js",
    "path": "./node_modules/path-browserify/index.js"
  }
}
